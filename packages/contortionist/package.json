{
  "name": "contortionist",
  "type": "module",
  "version": "0.0.8",
  "repository": {
    "type": "git",
    "url": "https://github.com/thekevinscott/contortionist.git"
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "scripts": {
    "lint": "wireit",
    "test": "vitest",
    "build": "wireit"
  },
  "wireit": {
    "lint": {
      "command": "eslint -c .eslintrc.cjs src --ext .ts"
    },
    "build": {
      "command": "rimraf ./dist && vite build && cp ../../README.md ./README.md",
      "files": [
        "./src/**/*.json",
        "./src/**/*.ts",
        "./src/**/*.js",
        "package.json",
        "vite.config.ts",
        "tsconfig.json"
      ],
      "output": [
        "./dist/**"
      ]
    }
  },
  "author": "Kevin Scott",
  "license": "MIT",
  "dependencies": {
    "@xenova/transformers": "^2.16.0",
    "gbnf": "latest"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "@types/jest": "^29.5.12",
    "rimraf": "^5.0.5",
    "rollup": "^4.12.1",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-string": "^3.0.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.5",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.3.1"
  }
}
